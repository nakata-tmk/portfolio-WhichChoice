<%= render partial: 'layouts/message', locals: { model: @genre } %>


<h2>ジャンル編集</h2>

<%= form_with model: [:admin, @genre], local:true do |f| %>
  <table class="table table-borderless">
    <tr>
      <td>ジャンル名</td>
      <td><%= f.text_field :name %></td>
      <td><%= f.submit '変更を保存', class: "btn btn-success text-white" %></td>
    </tr>
  </table>
<% end %>